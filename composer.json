{
    "name": "bondacom/laravel-hashids",
    "description": "Laravel package to hash ids",
    "license": "MIT",
    "keywords": ["api", "id", "hash"],
    "require": {
        "php": "^7.1",
        "hashids/hashids": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Bondacom\\LaravelHashids\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bondacom\\LaravelHashids\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bondacom\\LaravelHashids\\Providers\\LaravelHashidsServiceProvider"
            ]
        }
    }
}
